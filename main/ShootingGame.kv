#: kivy 1.9.1

<Timer>:
	Label:
		font_size: 30
		pos:(root.width-80,root.height+520)
		text:'Timer:'+str(root.timer)

<Enemy>:
	size:self.size
	pos:self.pos
	source:'data/ufo.png'

<Rocket>:
	size:self.size
	pos:self.pos
	source:'data/rocket.png'

<Ball>:
	size: 10, 10
	canvas:
		Ellipse:
		pos:self.pos
		size:self.size
		source:'data/ball.png'

<ShootingGame>:
	timer: timer_val
	fps:fps_val
	Timer:
		id:timer_val
		text:'Timer:'+str(root.timer.timer)

	Label:
		id:fps_val
		pos:(0,0)
		text:'fps:'+str(root.fps)
